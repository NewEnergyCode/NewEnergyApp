<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginEnd="10dp">


    <ImageView
        android:id="@+id/CategoryCD1"
        android:layout_width="117dp"
        android:layout_height="46dp"
        android:contentDescription="@string/CategoryCD"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle_11" />

    <TextView
        android:id="@+id/CategoryTitle"
        android:layout_width="118dp"
        android:layout_height="48dp"
        android:background="@drawable/bg_button_rounded"
        android:gravity="center"
        android:text="@string/CategoryTitle"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/CategoryCD1"
        app:layout_constraintStart_toStartOf="@+id/CategoryCD1"
        app:layout_constraintTop_toTopOf="@+id/CategoryCD1"
        app:layout_constraintVertical_bias="1.0" />
</androidx.constraintlayout.widget.ConstraintLayout>